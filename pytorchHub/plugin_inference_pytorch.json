{
    "name": "PytorchHub_Classification",
    "version": "0.1.0",
    "title": "Timm_Classification",
    "description": "Plugin to load classification models from PytorchHub",
    "author": "Berthold Chloé & Cliquot Théo",
    "institution": "ISIMA",
    "repository": "https://github.com/dummer3/PluginHFClassification",
    "website": "",
    "citation": "HELP ",
    "containerId": "c9986b01ad54",
    "baseCommand": [],
    "inputs": [
        {
            "name": "model-path",
            "type": "string",
            "description": "Model path for the PytorchHub of the classification model you want to use",
            "required": true
        },
        {
            "name": "image-folder",
            "type": "collection",
            "description": "Folder where the image you want to use are located",
            "required": true
        },
        {
            "name": "image-format",
            "type": "string",
            "description": "Format of the image you want to analyse",
            "required": false
        }
    ],
    "outputs": [
        {
            "name": "output-filepath",
            "type": "csvCollection",
            "description": "File path where our csv result will be store"
        }
    ],
    "ui": [
        {
            "key": "inputs.model-path",
            "title": "input-model",
            "description": "Choose your model",
            "hidden": false
        },
        {
            "key": "inputs.image-folder",
            "title": "image-folder",
            "description": "Image folder",
            "default": "data/inputs",
            "hidden": false
        },
        {
            "key": "inputs.image-format",
            "title": "image-format",
            "description": "(tif, jog, png ...) tif by default",
            "hidden": true
        }
    ],
    "resourceRequirements": {
        "cpuAVX": false,
        "cpuAVX2": false,
        "gpu": false
    }
}